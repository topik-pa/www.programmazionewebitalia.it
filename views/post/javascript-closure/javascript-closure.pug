extends ../../layout.pug

block append meta
  title #{title} | #{proj_name}
  meta(name="description" content="JavaScript CLOSURE: cosa sono e a cosa servono. Definizione, uso ed esempi di codice per capire meglio le Closure in JavaScript")
  meta(name="keywords" content="javascript, closure, esempi")
  link(rel="canonical", href=proj_url + url)

  meta(name="og:url" content=proj_url + url)
  meta(name="og:type" content="article")
  meta(name="og:title" content="JavaScript CLOSURE: cosa sono e a cosa servono")
  meta(name="og:site_name" content="PWI - Programmazione Web Italia")
  meta(name="og:description" content="PWI - Programmazione Web Italia")
  meta(name="og:image" content=proj_url + "/assets/images/post/post-js-closure-cover-min.png")

block main
  main(class="tworows")
    include ./javascript-closure-article.pug

    include ../../../components/shared/sideside/sideside.pug

