extends ../../layout.pug

block append meta
  title #{title} | #{proj_name}
  meta(name="description" content=title)
  meta(name="keywords" content="analisi, confronto, portali, ecommerce, moda, italia, tezenis, intimissimi, benetton, ovs, terranova")
  link(rel="canonical", href=proj_url + url)

  meta(name="og:url" content=proj_url + url)
  meta(name="og:type" content="article")
  meta(name="og:title" content=title)
  meta(name="og:site_name" content=proj_name)
  meta(name="og:description" content=proj_tagline)
  meta(name="og:image" content=proj_url + "/assets/images/analysis/confronto-portali-ecommerce-moda-italiani/cover-og-image.webp")

block main
  main(class="tworows")
    article
      include ./confronto-portali-ecommerce-moda-italiani-article.pug

      include /components/shared/coffee/coffee.pug

    include ../../../components/shared/side/side.pug

block viewscripts 
  if prod
    script(defer, src='https://platform.linkedin.com/in.js')
  script(type='application/ld+json').
    {
      "@context": "https://schema.org",
      "@type": "Article",
      "datePublished": "2024-06-06",
      "dateModified": "2024-06-06",
      "url": "#{proj_url + url}",
      "name": "Confronto tecnologico tra i maggiori portali di e-commerce di moda italiani",
      "headline": "#{title}",
      "description": "#{proj_description}",
      "image": "#{proj_url}/assets/images/analysis/confronto-portali-ecommerce-moda-italiani/cover-og-image.webp",
      "author": {
        "@type": "Person",
        "name": "#{owner}"
      },
      "copyrightHolder": {
        "@type": "Person",
        "name": "#{owner}"
      },
      "publisher": {
        "@type": "Organization",
        "name": "#{proj_name}",
        "url": "#{proj_url}",
        "logo": "#{logo}"
      }
    }